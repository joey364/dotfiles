{
	"name": "smart-css-autocomplete",
	"displayName": "Smart CSS Autocomplete",
	"description": "Autocomplete CSS properties smarter - using an usage statistics instead of standard random",
	"license": "MIT",
	"version": "1.4.2",
	"categories": [
		"Programming Languages"
	],
	"author": "Saionaro <shibakow@gmail.com>",
	"publisher": "saionaro",
	"repository": {
		"type": "git",
		"url": "https://github.com/Saionaro/smart-css-autocomplete.git"
	},
	"bugs": {
		"url": "https://github.com/Saionaro/smart-css-autocomplete/issues"
	},
	"icon": "icon.png",
	"keywords": [
		"autocomplete",
		"suggestions",
		"css",
		"less",
		"scss",
		"props",
		"properties"
	],
	"engines": {
		"vscode": "^1.43.0"
	},
	"activationEvents": [
		"onLanguage:css",
		"onLanguage:scss",
		"onLanguage:less"
	],
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -b",
		"typecheck": "tsc --noEmit",
		"watch": "tsc -b -w",
		"lint": "eslint --ext .ts src",
		"format": "prettier --write",
		"release": "semantic-release",
		"test": "jest",
		"dev-test": "jest --watch"
	},
	"dependencies": {
		"known-css-properties": "^0.19.0"
	},
	"devDependencies": {
		"@arkweid/lefthook": "^0.7.2",
		"@commitlint/cli": "^9.1.2",
		"@commitlint/config-conventional": "^9.1.2",
		"@semantic-release/git": "^9.0.0",
		"@types/jest": "^26.0.13",
		"@types/node": "^12.12.0",
		"@types/vscode": "1.43.0",
		"@typescript-eslint/eslint-plugin": "^3.10.1",
		"@typescript-eslint/parser": "^2.3.0",
		"babel-jest": "^26.3.0",
		"eslint": "^6.4.0",
		"eslint-config-prettier": "^6.11.0",
		"jest": "^26.4.2",
		"prettier": "^2.1.1",
		"semantic-release": "^17.1.1",
		"semantic-release-vsce": "^3.0.1",
		"ts-jest": "^26.3.0",
		"typescript": "^4.0.2"
	},
	"__metadata": {
		"id": "116224e0-c915-4307-b163-08119db0c0ec",
